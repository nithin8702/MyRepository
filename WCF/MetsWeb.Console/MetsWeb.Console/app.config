<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ILoanService" />
                <binding name="BasicHttpBinding_ITestService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost/myservice2/LoanService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ILoanService"
                contract="LoanServiceReference.ILoanService" name="BasicHttpBinding_ILoanService" />
            <endpoint address="http://localhost/myservice2/TestService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ITestService"
                contract="TestServiceReference.ITestService" name="BasicHttpBinding_ITestService" />
        </client>
    </system.serviceModel>
</configuration>